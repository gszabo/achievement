{% extends 'AchieFrontendBundle:Default:base.html.twig' %}

{% block content %}
    <h1>Felhasználóink</h1>

    <table class="records_list">
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('achie_frontend_user_show', { 'id': entity.id }) }}">{{ entity.username }}</a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {% endblock %}
