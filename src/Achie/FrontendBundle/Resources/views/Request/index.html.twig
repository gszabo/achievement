{% extends 'AchieFrontendBundle:Default:base.html.twig' %}

{% block content %}
    <h1>Request list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Createdby</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>Updatedby</th>
                <th>Name</th>
                <th>Description</th>
                <th>Picture</th>
                <th>Level</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('achie_frontend_request_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.createdBy }}</td>
                <td>{% if entity.createdAt %}{{ entity.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.updatedAt %}{{ entity.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.updatedBy }}</td>
                <td>{{ entity.name }}</td>
                <td>{{ entity.description }}</td>
                <td>{{ entity.picture }}</td>
                <td>{{ entity.level }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('achie_frontend_request_show', { 'id': entity.id }) }}">show</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {% endblock %}
